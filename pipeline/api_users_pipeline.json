{
	"name": "api_users_pipeline",
	"properties": {
		"activities": [
			{
				"name": "copy_api_users_csv_adls",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['page']"
								},
								"sink": {
									"name": "page"
								}
							},
							{
								"source": {
									"path": "$['per_page']"
								},
								"sink": {
									"name": "per_page"
								}
							},
							{
								"source": {
									"path": "$['total']"
								},
								"sink": {
									"name": "total"
								}
							},
							{
								"source": {
									"path": "$['total_pages']"
								},
								"sink": {
									"name": "total_pages"
								}
							},
							{
								"source": {
									"path": "['id']"
								},
								"sink": {
									"name": "id"
								}
							},
							{
								"source": {
									"path": "['email']"
								},
								"sink": {
									"name": "email"
								}
							},
							{
								"source": {
									"path": "['first_name']"
								},
								"sink": {
									"name": "first_name"
								}
							},
							{
								"source": {
									"path": "['last_name']"
								},
								"sink": {
									"name": "last_name"
								}
							},
							{
								"source": {
									"path": "['avatar']"
								},
								"sink": {
									"name": "avatar"
								}
							},
							{
								"source": {
									"path": "$['support']['url']"
								},
								"sink": {
									"name": "url"
								}
							},
							{
								"source": {
									"path": "$['support']['text']"
								},
								"sink": {
									"name": "text"
								}
							}
						],
						"collectionReference": "$['data']",
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "Json_api_users",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "api_users",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}